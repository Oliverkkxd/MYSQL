CREATE TABLE games (
  id_games int auto_increment not null,
  nome varchar(250),
  ano_lancamento tinyint(4),
  genero varchar(15),
  primary key(id_games)
);


CREATE TABLE usuarios(
  id_user int auto_increment not null,
  nome_user varchar(250),
  data_nasc varchar(10),
  game_fav varchar(15)
);

desc games;
